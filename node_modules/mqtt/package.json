    "typescript": "^3.2.2",
    "uglify-js": "^3.4.5",
    "ws": "^3.3.3",
    "zuul": "^3.12.0",
    "zuul-ngrok": "^4.0.0"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
